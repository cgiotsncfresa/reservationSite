<p id="notice"><%= notice %></p>

<div class="well">
<h1>Liste des r√©servations</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Desk</th>
      <th>Identifiant de l'appareil</th>
      <th>Description</th>
      <th>Reservation date</th>
      <th>Reservation pm</th>
      <th>Reservation am</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
   
    <% @reservations.each do |reservation| %>
     <%=debug reservation %>  
      <tr>
        <td><%= reservation.desk_id %></td>
        <td></td>
        <td><%= reservation.description %></td>
        <td><%= reservation.reservation_date %></td>
        <td><%= reservation.reservation_pm %></td>
        <td><%= reservation.reservation_am %></td>
        <td><%= link_to 'Show', reservation %></td>
        <td><%= link_to 'Edit', edit_reservation_path(reservation) %></td>
        <td><%= link_to 'Destroy', reservation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
<br>

<%= link_to 'New Reservation', new_reservation_path %>
